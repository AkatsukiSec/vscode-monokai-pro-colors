        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
    "editor.tokenColorCustomizations": {
        
        "textMateRules": [
            {
                "scope": "meta.function-call.generic.python",
